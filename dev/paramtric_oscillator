import numpy as np
import matplotlib.pyplot as plt

steps = 5000
x = np.zeros(steps)
y = np.zeros(steps)

# Constant parameters
a = 2.0
b = 2.5

# Pure oscillator without time modulation
for i in range(1, steps):
    x[i] = np.sin(a * y[i-1])
    y[i] = np.cos(b * x[i-1])

plt.plot(x, y, '.', markersize=5)
plt.title("Regular Parametric Oscillator")
plt.axis('equal')
plt.show()




